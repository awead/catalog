<%= content_tag :div, :class => "row" do %>
  <%= content_tag :div, :class => "col-sm-2 col-sm-offset-10" do %>
    <%= content_tag :label, :class => "checkbox" do %>
      <%= check_box_tag "toggle_all_bookmarks" %>
      <%= content_tag :span, "Select All" %>
    <% end %>
  <% end %>
<% end %>

<%= content_tag :div, :id => "documents" do %>
  <%= render documents, :as => :document %>
<% end %>