<% element.xpath('chronlist').each do |chronlist| %>
  <% if !chronlist.xpath('head').first.blank? -%>
    <h4>
      <%= chronlist.xpath('head').first.text  %>
    </h4>
  <% end -%>
  <table>
    <% chronlist.xpath('chronitem').each do |chronitem| %>
      <tr>
        <td>
          <%= chronitem.xpath('date').first %>
        </td>
        <td>
          <%= chronitem.xpath('event').first %>
        </td>
      </tr>
    <% end %>
  </table>

<% end %>
